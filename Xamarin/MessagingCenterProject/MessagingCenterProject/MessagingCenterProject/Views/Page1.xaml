<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MessagingCenterProject.Views.Page1">
    <ContentPage.Content>
        <StackLayout>
            <Entry Placeholder="Topic" x:Name="entryTopic"></Entry>
            <Entry Placeholder="Content" x:Name="entryContent"></Entry>
            <Button Text="Send" Clicked="Button_Clicked"></Button>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>